function CountPorts(ini, format) {
   var fso = new ActiveXObject("Scripting.FileSystemObject");
   var oFile = fso.OpenTextFile(ini, 1);
   var sTemp = oFile.ReadAll().toLowerCase();
   oFile.Close();
   var found = CountMatch(sTemp, format.toLowerCase());
   return found;
}

