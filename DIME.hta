function SaveAs() {
   document.getElementById("overlayText").innerHTML = "";
   document.getElementById("overlay").style.display = "block";

   var excel = new ActiveXObject("Excel.Application");
   var fso = new ActiveXObject("Scripting.FileSystemObject");
   var shell = new ActiveXObject("Shell.Application");
   var saveAsTitle = DIME.applicationName + " Save As..."

   //Excel SaveAs dialog will always open in the background, this function handles it
   BringToFront(saveAsTitle);
   var path = excel.GetSaveAsFileName(fso.GetAbsolutePathName(""), "Text files (*.txt;*.dmc),*.txt;*.dmc,All files (*.*),*.*", 1, saveAsTitle);

   document.getElementById("overlay").style.display = "none";

   if (!path) { return false; }
   if (fso.fileExists(path)) {
      var rsp = vMsgBox("File \"" + path + "\" already exists.\r\nDo you want to replace it?", 1, "Overwrite?");
      if (rsp == 2) { return false; } //exit if cancelled
   }
   var ts = fso.OpenTextFile(path,2,true);
   ts.Write(NS.myText.value.replace(/\n/g,"\r\n"));
   ts.Close();
   NS.FileName = path;
   DisplayTitle();
   Modified(false);
   return true;
}

// This function creates a .vbs that:
//   1.) Tries to use .AppActivate to bring the app to the front
//   2.) Loops until the app is activated or until 2 seconds have passed
//   3.) Deletes itself
function BringToFront(app) {
   var wScript = new ActiveXObject("WScript.Shell");
   var fso = new ActiveXObject("Scripting.FileSystemObject");
   var f = fso.CreateTextFile(NS.Path + "temp.vbs");
   f.Write(["Dim objShell: Set objShell = CreateObject(\"Wscript.Shell\")",
                "dim success: success = false",
                "dim sTime: sTime = Timer",
                "do while success = false",
                "   if ((Timer - sTime) > 2) then exit do",
                "   success = ObjShell.AppActivate(\"" + app + "\")",
                "loop",
                "CreateObject(\"Scripting.FileSystemObject\").deleteFile(WScript.ScriptFullName)",
                "WScript.Quit"].join("\r\n"));
   f.Close();
   wScript.Run(NS.Path + "temp.vbs", 0);
}

// Send Keys after delay by creating a .vbs that uses send keys with code to delete itself after
// NOTE: This shouldn't be used
function SendKeys(keys, delay) {
   var wScript = new ActiveXObject("WScript.Shell");
   var fso = new ActiveXObject("Scripting.FileSystemObject");
   var f = fso.CreateTextFile(NS.Path + "temp.vbs");
   f.Write(["WScript.Sleep " + delay,
                "CreateObject(\"WScript.Shell\").sendKeys(\"" + keys + "\")",
                "CreateObject(\"Scripting.FileSystemObject\").deleteFile(WScript.ScriptFullName)",
                "WScript.Quit"].join("\r\n"));
   f.Close();
   wScript.Run(NS.Path + "temp.vbs", 0);
}

