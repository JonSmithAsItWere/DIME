}

function showMainIOTab() {
   var co, i, j, k;
   var frm = document.getElementById("MyFrame").contentDocument;
   document.getElementById("resetWatch").disabled = true;
   frm.getElementById("varWatch").style.display = "none";
   frm.getElementById("mainIO").style.display = "inline-block";
   frm.getElementById("inputs").style.display = "none";
   frm.getElementById("outputs").style.display = "none";
   ConfigureExtendedIO();
   document.getElementById("pollTime").value = NS.mainIOPoll;
   UpdatePoll();
}

function ConfigureExtendedIO() {
   var co, i, j, k;
   try {
      co = NS.galil.commandValue("MG_CO");
   } catch(e) {
      co = 0;
   }
   if (NS.co == co) { return; }
   for (i = 1; i <= 4; i++) {
      for (j = 1; j <= 8; j++) {
         k = ((i + 1) * 8) + j;
         if (co & Math.pow(2, i - 1)) {
            document.getElementById("MyFrame").contentDocument.getElementById("EXT" + k).disabled = false;
         } else {
            document.getElementById("MyFrame").contentDocument.getElementById("EXT" + k).disabled = true;
         }
      }
   }
   NS.co = co;
}

